@model NIA_CRM.Models.Cancellation

@{
    ViewData["Title"] = "Delete";
}

<partial name="_Notification" />



<h1>Activate</h1>

<h3>Are you sure you want to activate this member?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Archived Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CancellationDate)
        </dd>
        <dt class = "col-sm-2">
            Archived
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsCancelled)
        </dd>
        <dt class = "col-sm-2">
            Archiving Note
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CancellationNote)
        </dd>
        <dt class = "col-sm-2">
           Member
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Member.MemberName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Activate" class="btn btn-danger" /> |
        <a asp-action="Index">Back to Member Archivings</a>
    </form>
</div>
